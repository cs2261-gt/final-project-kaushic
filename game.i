   }
   if (cheat == 1 && collision(e->col, e->row, e->width, e->height,
          doctor.col, doctor.row, doctor.width, doctor.height)){
    e->hitsTaken += 1;
    if (e->col == 240){
     e->active = 0;
     activeEnemies -= 1;
    } else {
     e->cdel *= -1;
     if (e->col < doctor.col){
      e->col -= 5;
     } else {
      e->col += 5;
     }
    }
   }
  }
 }
}

