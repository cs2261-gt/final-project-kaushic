   shadowOAM[20 + i].attr0 = (0xFF & powerups[i].row) | (0<<13) | (0<<14) | (1<<10);
   shadowOAM[20 + i].attr1 = (0x1FF & powerups[i].col) | (2<<14);
   shadowOAM[20 + i].attr2 = ((r)*32+(c));
  } else {
   shadowOAM[20 + i].attr0 = (2<<8);
  }
 }
}

